{
  "expo": {
    "name": "PLNR",
    "slug": "plnr-guide-mobile",
    "scheme": "plnr",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f4f4f5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.soru.events.tripplannermobile",
      "associatedDomains": [
        "applinks:plnr.guide",
        "applinks:www.plnr.guide"
      ]
    },
    "android": {
      "package": "com.soru.events.tripplannermobile",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "plnr.guide",
              "pathPrefix": "/auth-callback"
            },
            {
              "scheme": "https",
              "host": "www.plnr.guide",
              "pathPrefix": "/auth-callback"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-foreground.png",
        "backgroundColor": "#5f6a6a",
        "monochromeImage": "./assets/adaptive-monochrome.png"
      }
    }
  }
}
